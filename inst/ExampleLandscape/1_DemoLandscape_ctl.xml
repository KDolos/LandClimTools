<?xml version="1.0" encoding="utf-8"?>
<landclimInput>
    <inputPath/>
    <speciesAttributes>3_LandClim_31species_CenEurope.xml</speciesAttributes>
    <treeInitState></treeInitState>
    <landMask>Mask_demo.txt</landMask>
    <soilDepth>Soil_demo.txt</soilDepth>
	<availableNitrogen>Soil_demo.txt</availableNitrogen>
    <slope>Slope_demo.txt</slope>
    <aspect>Aspect_demo.txt</aspect>
    <elevation>Dem_demo.txt</elevation>
    <landtype>Landtype_demo.txt</landtype>    
    <landtypePars>2_DemoLandscape_landtypes.xml</landtypePars>
    <climate>Dischma_Climate_random.dat</climate>
    <disturbance>
    	<fire>
    		<active>0</active>
    		<fireProbExp>2.5</fireProbExp>
    		<fireProbExpBeta>1.5</fireProbExpBeta>
    		<fireProbExpPMax>1.0</fireProbExpPMax>
    		<fireProbExpPMin>0.2</fireProbExpPMin>
    		<fireSizeMax>20</fireSizeMax>
    	</fire>
      	<wind>
      		<active>0</active>
			<type>STANDARD</type>
    		<breakoutCoefficient>0.002</breakoutCoefficient>
			<meanWindthrowSize>1250</meanWindthrowSize>
			<probabilityCoefficient>200</probabilityCoefficient>
			<sizeCoefficient>0.2</sizeCoefficient>
			<meanReturnInterval>800</meanReturnInterval>
			<minWindthrowSize>620</minWindthrowSize>
			<maxWindthrowSize>2500</maxWindthrowSize>
    	</wind>
		<harvest>
    		<active>0</active>
    		<harvestPars></harvestPars>
    		<harvestManagementAreas></harvestManagementAreas>
    		<harvestStands></harvestStands>
    	</harvest>
    	<planting>
    		<active>0</active>
    	</planting>
		<beetle>
            <active>0</active>
            <betaScale>10000</betaScale>
            <iniBDI>0.01</iniBDI>
            <bgBPI>0.01</bgBPI>
            <siWMaxCoeff>1</siWMaxCoeff>
            <bdiMaxCoeff>1</bdiMaxCoeff>
            <cellCoI>1</cellCoI>
            <siCellNI>0</siCellNI>
            <siWNI>1</siWNI>
            <wISiCell>0.5</wISiCell>
            <BPCoeff>14</BPCoeff>
            <maxInfBm>150</maxInfBm>
            <wIDist>200</wIDist>
            <dTBark>2</dTBark>
        </beetle>
    </disturbance>
    <output>
    	<path>.\Artificial_Input\Test_Output</path>
	<aggregation>
			<set>
				<name>elevation</name>
				<map>Dem_demo.txt</map>
			</set>
			<set>
				<name>landtype</name>
				<map>Landtype_demo.txt</map>
			</set>
		</aggregation>
 		<cohort>
			<aggregation>			
				<name>landtype</name>
				<name>elevation</name>					
			</aggregation>
      		<biomass>
      			<start>0</start>
        		<interval>0</interval>
        	</biomass>
        	<stemCount>
        		<start>0</start>
        		<interval>0</interval>
        	</stemCount>
    		<full>
    			<decades>1 5 10</decades>
    		</full>
 		</cohort>
	</output>
    <dispersal>
		<mode>UNIFORM</mode>
    </dispersal>
	<debug>
				<cohortObserver>0</cohortObserver>
				<climateData>0</climateData>
				<regenerationLocal>0</regenerationLocal>
	</debug>
	<waterBalanceModel>ORIGINAL</waterBalanceModel>
    <iterations>11</iterations>
	<randomNumberSeed>
	  <x>1</x>
	  <y>1</y>
	  <z>1</z>
    </randomNumberSeed>
</landclimInput>
